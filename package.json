{
    "name": "angular-electron",
    "version": "13.0.0",
    "description": "Angular 16 with Electron 25 (Typescript + SASS + Hot Reload)",
    "homepage": "https://github.com/maximegris/angular-electron",
    "author": {
        "name": "Maxime GRIS",
        "email": "maxime.gris@gmail.com"
    },
    "keywords": [
        "angular",
        "angular 16",
        "electron",
        "electron 27",
        "nodejs",
        "typescript",
        "playwright",
        "eslint",
        "sass",
        "windows",
        "mac",
        "linux"
    ],
    "main": "app/main.js",
    "private": true,
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "ng": "ng",
        "start": "npm-run-all -p electron:serve ng:serve",
        "ng:serve": "ng serve -c web",
        "build:dev": "npm run build -- -c dev",
        "build:prod": "npm run build -- -c production",
        "web:build": "npm run build -- -c web-production",
        "electron": "DISPLAY=:0 electron",
        "electron:serve-tsc": "tsc -p tsconfig.serve.json",
        "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && DISPLAY=:0 electron . --serve",
        "electron:local": "npm run build:prod && DISPLAY=:0 electron .",
        "electron:build": "npm run build:prod && electron-builder build --publish=never",
        "test": "ng test --watch=false",
        "test:watch": "ng test",
        "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
        "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
        "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "lint": "ng lint",
        "eslint-check": "eslint .",
        "lint:css": "stylelint '**/*.scss' --fix",
        "format:css": "prettier --write '**/*.scss'",
        "format-and-lint:css": "npm run format:css && npm run lint:css",
        "rebuild-sqlite": "electron-rebuild -f -w sqlite3",
        "electron:rebuild": "node_modules/.bin/electron-rebuild && npm run rebuild-sqlite",
        "clean-reset": "rm -rf node_modules && npm cache clean --force && npm install",
        "web": "npm run eslint-check && npm run build-angular && npm run ng:serve:windows && npm run server",
        "server": "node server/server.js",
        "ng:serve:windows": "fuser -k 4200/tcp || true; ng serve -c web",
        "build:preload": "python /home/matheus/programming/archencil-angular-16/scripts/package.json/update_preload_fonts.py",
        "build:svg-json-list": "python /home/matheus/programming/archencil-angular-16/scripts/package.json/update_svg_list.py",
        "build": "npm run electron:serve-tsc && ng build --base-href ./",
        "build-angular": "tsc --noEmit && ng build --base-href ./ && stylelint '**/*.scss' --config .stylelintrc.json && npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet",
        "local": "npm run eslint-check && npm run build-angular && npm run electron:local"
    },
    "dependencies": {
        "@angular/animations": "16.1.3",
        "@angular/common": "16.1.3",
        "@angular/compiler": "16.1.3",
        "@angular/core": "16.1.3",
        "@angular/forms": "16.1.3",
        "@angular/language-service": "16.1.3",
        "@angular/platform-browser": "16.1.3",
        "@angular/platform-browser-dynamic": "16.1.3",
        "@angular/router": "16.1.3",
        "@ngrx/store": "16.3.0",
        "@sweetalert2/ngx-sweetalert2": "12.2.0",
        "@tinymce/tinymce-angular": "7.0.0",
        "@types/fontfaceobserver": "2.1.3",
        "dotenv": "16.3.1",
        "fabric": "5.3.0",
        "fontfaceobserver": "2.3.0",
        "ngx-tippy-wrapper": "6.2.2",
        "ngx-toastr": "17.0.2",
        "rxjs": "7.8.1",
        "sqlite3": "5.1.6",
        "sweetalert2": "11.9.0",
        "tailwindcss": "3.3.5",
        "tslib": "^2.5.0",
        "zone.js": "~0.13.1"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "16.0.0",
        "@angular-devkit/build-angular": "16.1.3",
        "@angular-eslint/builder": "16.0.3",
        "@angular-eslint/eslint-plugin": "16.0.3",
        "@angular-eslint/eslint-plugin-template": "16.0.3",
        "@angular-eslint/schematics": "16.0.3",
        "@angular-eslint/template-parser": "16.0.3",
        "@angular/cli": "16.1.3",
        "@angular/compiler-cli": "16.1.3",
        "@ngx-translate/core": "15.0.0",
        "@ngx-translate/http-loader": "8.0.0",
        "@playwright/test": "1.35.1",
        "@types/fabric": "5.3.6",
        "@types/jasmine": "4.3.1",
        "@types/jasminewd2": "2.0.10",
        "@types/lodash": "4.14.201",
        "@types/node": "18.16.9",
        "@types/sqlite3": "3.1.11",
        "@typescript-eslint/eslint-plugin": "5.60.1",
        "@typescript-eslint/parser": "5.60.1",
        "conventional-changelog-cli": "3.0.0",
        "electron": "27.1.0",
        "electron-builder": "24.4.0",
        "electron-debug": "3.2.0",
        "electron-rebuild": "3.2.9",
        "electron-reloader": "1.2.3",
        "eslint": "8.43.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsdoc": "46.4.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "jasmine-core": "4.6.0",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.4.2",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-electron": "7.3.0",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.0.0",
        "node-abi": "3.51.0",
        "node-polyfill-webpack-plugin": "2.0.1",
        "npm-run-all": "4.1.5",
        "playwright": "1.35.1",
        "prettier": "3.0.3",
        "string-replace-loader": "3.1.0",
        "stylelint": "15.11.0",
        "stylelint-config-css-modules": "4.3.0",
        "stylelint-config-standard-scss": "11.1.0",
        "stylelint-prettier": "4.0.2",
        "ts-node": "10.9.1",
        "typescript": "~4.9.5",
        "wait-on": "7.0.1",
        "webdriver-manager": "12.1.9"
    },
    "engines": {
        "node": ">= 16.14.0 || >= 18.10.0"
    },
    "browserslist": [
        "chrome 114"
    ]
}
